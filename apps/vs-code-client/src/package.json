{
  "name": "dyvil-vscode",
  "publisher": "dyvil",
  "author": {
    "name": "Adrian Kunz",
    "email": "clashsoft@hotmail.com"
  },
  "version": "0.0.0",
  "main": "main.js",
  "engines": {
    "vscode": "^1.74.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "dyvil.restart",
        "title": "Restart Dyvil Language Server"
      }
    ],
    "languages": [
      {
        "id": "dyvil",
        "aliases": [
          "Dyvil"
        ],
        "extensions": [
          ".dyv"
        ]
      }
    ],
    "grammars": [
      {
        "language": "dyvil",
        "scopeName": "source.dyvil",
        "path": "./assets/dyvil.tmGrammar.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:dyvil"
  ]
}
